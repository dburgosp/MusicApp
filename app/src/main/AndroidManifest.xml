<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.david.musicapp">

    <application
        android:allowBackup="true"
        android:fullBackupContent="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".RowListActivity"
            android:label="@string/title_songs_list" />
        <activity
            android:name=".ColumnListActivity"
            android:label="@string/title_options_list" />
        <activity
            android:name=".NowPlayingActivity"
            android:label="@string/title_now_playing" />
        <activity android:name=".SearchActivity"></activity>
    </application>

</manifest>